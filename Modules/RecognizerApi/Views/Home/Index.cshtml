<div class="row">
    <div class="form-group">
        <label>Strokes:</label>
        <input class="form-control" type="text" id="txtStroke" value="1" placeholder="Enter Stroke" required />
    </div>
    <div class="form-group">
        <input id="lblResult" class="form-control" type="text" readonly />
        <label>Result as Json:</label>
        <textarea class="form-control xmlshow"></textarea>
    </div>
    <button class="btn btn-primary btn-large" onclick="GetFraz()">Request</button>

</div>
<script>
    function GetFraz() {
        var points = "11,0,11,0,11,0,12,0,12,4,14,10,16,16,17,20,19,27,20,31,22,35,22,36,22,37,23,38,25,38,27,37,28,35,30,34,32,32,33,32,35,31,35,30,36,30,37,30,38,30,41,32,43,34,45,37,45,39,48,42,48,43,48,45,48,47,49,49,49,51,48,55,46,57,43,61,38,65,31,68,22,72,17,74,11,75,8,75,4,77,2,77,1,77,0,77";
        //$('#txtStroke').val();

        $('#lblResult').val('Waiting...');
        var url = "http:/api/Mip/GetFraz";
        $.ajax({
            type: 'GET',
            url: url,
            data: { points: points }
        }).done(function (result) {
            var strJ = JSON.stringify(result);
            $('#lblResult').val('Done');
            $(".xmlshow").html(strJ);
        })
    }
</script>